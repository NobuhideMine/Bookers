<h1><strong>Books</strong></h1>

<table>
    <tr>
        <th>Title</th>
        <th>Body</th>
    </tr>
    
    <td><%= link_to book.title, "/book/#{book.id}" %></td>rai
</table>














<h2>New book</h2>

<%= form_with model: @book do |f| %>
    
    <h4>Title</h4>
    <%= f.text_field :title %>
    
    <h4>Body</h4>
    <%= f.text_area :body %>
    
    <%= f.submit 'Create Book' %>
        
        
<% end %>